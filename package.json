{
  "name": "test-storage",
  "version": "0.3.40",
  "description": "Test Case Management System",
  "main": "app.js",
  "private": true,
  "scripts": {
    "prestart": "npm run env NODE_ENV=development",
    "start": "concurrently \"tsc -w -p ./client\" \"nodemon app.js --exitcrash\" ",
    "prestarttest": "concurrently \"tsc -p ./client\" \"tsc -p ./test\" ",
    "starttest": "nodemon app.js --exitcrash",
    "forever": "concurrently \"tsc -w -p ./client\" \"forever start -c nodemon app.js --exitcrash\" ",
    "version": "versiony package.json --patch",
    "testserver": "./node_modules/.bin/mocha ./test/server/ --recursive --check-leaks",
    "test": "./node_modules/.bin/mocha ./test/server/ --recursive --check-leaks & npm run protractor",
    "webdriverupdate": "./node_modules/.bin/webdriver-manager update",
    "webdriverstart": "./node_modules/.bin/webdriver-manager start",
    "protractor": "npm run prestarttest & sleep 10 & npm run starttest & sleep 10 && protractor ./test/protractor.conf.js --suite all",
    "postinstall": "./node_modules/.bin/typings install",
    "typings": "typings",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lint": "tslint 'client/**/*.ts'",
    "bundle:prod": "node tools/builder.js --prod"
  },
  "keywords": [
    "test management",
    "test case management"
  ],
  "author": "Dmitry Zakharov",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "dependencies": {
    "@angular/common": "^2.2.4",
    "@angular/compiler": "^2.2.4",
    "@angular/core": "^2.2.4",
    "@angular/forms": "^2.2.4",
    "@angular/http": "^2.2.4",
    "@angular/platform-browser": "^2.2.4",
    "@angular/platform-browser-dynamic": "^2.2.4",
    "@angular/router": "^3.2.4",
    "@angular/upgrade": "^2.2.4",
    "angular-in-memory-web-api": "^0.1.16",
    "angular2-jwt": "^0.1.25",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "errorhandler": "^1.5.0",
    "express": "^4.14.0",
    "express-validator": "^3.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "jwt-simple": "^0.5.1",
    "method-override": "^2.3.7",
    "mocha": "^3.2.0",
    "mongoose": "^4.7.1",
    "morgan": "^1.7.0",
    "normalize-css": "^2.3.1",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "^2.3.2",
    "systemjs": "^0.19.41",
    "yarn": "^0.17.9",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/core-js": "^0.9.35",
    "@types/es6-promise": "0.0.32",
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.51",
    "@types/protractor": "^4.0.0",
    "@types/should": "^8.1.30",
    "@types/should-promised": "0.0.27",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codelyzer": "^2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "factory-girl": "^4.0.0-beta.7",
    "faker": "^3.1.0",
    "forever": "^0.15.3",
    "mocha": "^3.2.0",
    "mockgoose": "^6.0.8",
    "nodemon": "^1.11.0",
    "protractor": "^4.0.11",
    "should": "^11.1.1",
    "supertest": "^2.0.1",
    "systemjs-builder": "^0.15.34",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10",
    "typings": "^2.0.0",
    "versiony-cli": "^1.3.0",
    "webpack": "^1.13.3",
    "yargs": "^6.4.0",
    "yarn": "^0.17.9"
  }
}
