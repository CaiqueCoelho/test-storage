{
  "name": "test-storage",
  "version": "0.27.0",
  "description": "Test Case Management System",
  "license": "GPLv3",
  "author": "Dmitry Zakharov",
  "private": true,
  "keywords": [
    "test management",
    "test case management"
  ],
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "set NODE_ENV=production&& node app.js",
    "postinstall": "./node_modules/.bin/ng build --prod --aot --no-progress",
    "server:run": "nodemon",
    "hmr": "concurrently \"npm run server:run \" \"ng serve --hmr -e=hmr --progress=false --proxy-config proxy.conf.json\" ",
    "dev": "concurrently \"npm run server:run \" \"ng serve --progress=false --proxy-config proxy.conf.json\"  ",
    "lint": "ng lint --type-check",
    "test": "ng test --single-run --no-progress && npm run test:backend",
    "test:frontend": "./node_modules/.bin/istanbul cover ng test --single-run --no-progress",
    "test:backend": "./node_modules/.bin/_mocha ./test/server/**/*.spec.ts --compilers ts:ts-node/register --recursive --check-leaks -t 50000",
    "test:coverage": "ng test --code-coverage",
    "pree2e": "webdriver-manager update",
    "e2e": "ng e2e --no-progress --proxy-config proxy.conf.json",
    "build": "npm run client:build && npm run server:build",
    "server:build": "tsc -p ./server",
    "client:build": "ng build --prod --aot",
    "extract": "ngx-translate-extract --input ./src --output ./i18n/*.json --clean --sort --format namespaced-json"
  },
  "dependencies": {
    "@angular/animations": "^4.3.6",
    "@angular/cli": "1.3.2",
    "@angular/common": "^4.3.6",
    "@angular/compiler": "^4.3.6",
    "@angular/compiler-cli": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/router": "^4.3.6",
    "@angularclass/hmr": "^2.1.3",
    "@ngx-translate/core": "7.2.0",
    "@ngx-translate/http-loader": "1.0.2",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.5",
    "@types/errorhandler": "0.0.31",
    "@types/express": "^4.0.37",
    "@types/jasmine": "2.5.54",
    "@types/jasminewd2": "~2.0.2",
    "@types/mocha": "2.2.42",
    "@types/morgan": "^1.7.32",
    "@types/node": "~8.0.25",
    "@types/should": "^11.2.0",
    "@types/supertest": "^2.0.3",
    "angular-tree-component": "^4.1.0",
    "angular2-jwt": "^0.2.3",
    "angular2-notifications": "^0.7.7",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "bootstrap": "^4.0.0-beta",
    "compression": "^1.7.0",
    "config": "^1.26.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.0",
    "database-updates": "^2.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "express-validator": "^3.2.1",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^7.4.3",
    "mongoose": "^4.11.9",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "ngx-bootstrap": "^1.9.1",
    "ngx-clickout": "^1.0.2",
    "nodemon": "^1.11.0",
    "normalize-css": "^2.3.1",
    "rxjs": "^5.4.3",
    "ts-node": "~3.3.0",
    "typescript": "~2.4.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/language-service": "^4.3.6",
