{
  "name": "test-storage",
  "version": "0.41.0",
  "description": "Test Case Management System",
  "license": "GPLv3",
  "author": "Dmitry Zakharov",
  "private": true,
  "keywords": [
    "test management",
    "test case management"
  ],
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "scripts": {
    "lint": "ng lint",
    "clean": "rimraf node_modules package-lock.json",
    "prebuild": "ts-node --project ./server/tsconfig.json ./server/modules/jwt-init.ts",
    "build": "ng build --prod && tsc -p ./server",
    "test": "ng test --watch=false --no-progress && npm run test:backend",
    "test:frontend": "./node_modules/.bin/istanbul cover ng test --watch=false --no-progress",
    "test:backend-e2e": "./node_modules/.bin/_mocha ./test/server/**/projects-.spec.ts --require ts-node/register --recursive --check-leaks -t 50000 --exit",
    "test:coverage": "ng test --code-coverage",
    "pree2e": "webdriver-manager update",
    "e2e": "concurrently \"nodemon \" \"ng e2e --configuration ci\" ",
    "start": "set NODE_ENV=production&&node ./dist/server/main.js",
    "hmr": "concurrently \"nodemon \" \"ng serve --configuration hmr --progress=false\" ",
    "dev": "concurrently \"nodemon \" \"ng serve --progress=false\"  ",
    "extract": "ngx-translate-extract --input ./src --output ./i18n/*.json --clean --sort --format namespaced-json",
    "heroku-prebuild": "npm i -g @angular/cli ts-node",
    "heroku-postbuild": "npm run build",
    "ng": "ng",
    "lint:backend": "tslint -p ./server/tsconfig.json -c tslint.json",
    "test:backend": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm test"
    }
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@angular/animations": "^8.2.12",
    "@angular/common": "^8.2.12",
    "@angular/compiler": "^8.2.12",
    "@angular/compiler-cli": "^8.2.12",
    "@angular/core": "^8.2.12",
    "@angular/forms": "^8.2.12",
    "@angular/platform-browser": "^8.2.12",
    "@angular/platform-browser-dynamic": "^8.2.12",
    "@angular/pwa": "^0.803.14",
    "@angular/router": "^8.2.12",
    "@angular/service-worker": "^8.2.12",
    "@angularclass/hmr": "^2.1.3",
    "@auth0/angular-jwt": "^3.0.0",
    "@clr/angular": "^2.3.0",
    "@clr/icons": "^2.3.0",
    "@clr/ui": "^2.3.0",
    "@nestjs/common": "^6.8.5",
    "@nestjs/core": "^6.8.5",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.8.5",
    "@nestjs/platform-socket.io": "^6.8.5",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/websockets": "^6.8.5",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@webcomponents/custom-elements": "^1.2.4",
    "angular2-notifications": "^2.0.0",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "compression": "^1.7.4",
    "config": "^3.2.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.3.4",
    "database-updates": "^2.0.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "normalize.css": "^8.0.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "swagger-ui-express": "^4.1.2",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.14",
    "@angular/cli": "^8.3.14",
    "@angular/language-service": "^8.2.12",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@nestjs/cli": "^6.11.1",
    "@nestjs/testing": "^6.8.5",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "0.0.35",
    "@types/express": "^4.17.1",
    "@types/faker": "^4.1.6",
    "@types/jasmine": "^3.4.4",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^24.0.18",
    "@types/mocha": "5.2.7",
    "@types/node": "~12.11.7",
    "@types/supertest": "^2.0.8",
    "@types/ws": "^6.0.3",
    "codelyzer": "~5.2.0",
    "concurrently": "^5.0.0",
    "faker": "^4.1.0",
    "husky": "^3.0.9",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.9.0",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "protractor": "~5.4.2",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "tsconfig-paths": "^3.9.0",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "~3.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "server",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
