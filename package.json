{
  "name": "test-storage",
  "version": "0.41.0",
  "description": "Test Case Management System",
  "license": "GPLv3",
  "author": "Dmitry Zakharov",
  "private": true,
  "keywords": [
    "test management",
    "test case management"
  ],
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "lint:backend": "tslint -p ./server/tsconfig.json -c tslint.json",
    "prebuild": "ts-node --project ./server/tsconfig.json ./server/modules/jwt-init.ts",
    "build": "ng build --prod && tsc -p ./server",
    "test": "jest",
    "test:backend": "jest -c ./jest-server.config.js",
    "start": "set NODE_ENV=production&&node ./dist/server/main.js",
    "hmr": "concurrently \"nodemon \" \"ng serve --configuration hmr --progress=false\" ",
    "dev": "concurrently \"nodemon \" \"ng serve --progress=false\"  ",
    "clean": "rimraf node_modules package-lock.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "extract": "ngx-translate-extract --input ./src --output ./i18n/*.json --clean --sort --format namespaced-json",
    "heroku-prebuild": "npm i -g @angular/cli ts-node",
    "heroku-postbuild": "npm run build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm test"
    }
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/pwa": "^0.803.20",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@angularclass/hmr": "^2.1.3",
    "@auth0/angular-jwt": "^3.0.1",
    "@clr/angular": "^2.3.3",
    "@clr/icons": "^2.3.3",
    "@clr/ui": "^2.3.3",
    "@nestjs/common": "^6.10.11",
    "@nestjs/core": "^6.10.11",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.10.11",
    "@nestjs/platform-socket.io": "^6.10.11",
    "@nestjs/swagger": "^4.0.9",
    "@nestjs/websockets": "^6.10.11",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@webcomponents/custom-elements": "^1.3.1",
    "angular2-notifications": "^2.0.0",
    "bcrypt": "^3.0.7",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "config": "^3.2.4",
    "core-js": "^3.5.0",
    "database-updates": "^2.0.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "normalize.css": "^8.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "swagger-ui-express": "^4.1.2",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "^8.3.20",
    "@angular/language-service": "^8.2.14",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@nestjs/cli": "^6.12.9",
    "@nestjs/testing": "^6.10.11",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.8",
    "@types/jest": "^24.0.23",
    "@types/node": "~12.12.19",
    "@types/supertest": "^2.0.8",
    "@types/ws": "^6.0.4",
    "codelyzer": "~5.2.0",
    "concurrently": "^5.0.1",
    "faker": "^4.1.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "protractor": "~5.4.2",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "~8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~5.20.1",
    "typescript": "~3.5.3"
  }
}
