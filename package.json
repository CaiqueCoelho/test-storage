{
  "name": "test-storage",
  "version": "0.2.38",
  "description": "Test Case Management System",
  "main": "app.js",
  "private": true,
  "scripts": {
    "prestart": "npm run env NODE_ENV=development",
    "start": "concurrently \"tsc -w -p ./client\" \"nodemon app.js --exitcrash\" ",
    "forever": "concurrently \"tsc -w -p ./client\" \"forever start -c nodemon app.js --exitcrash\" ",
    "version": "versiony package.json --patch",
    "test": "./node_modules/.bin/mocha ./test/ --recursive --check-leaks",
    "postinstall": "./node_modules/.bin/typings install",
    "typings": "typings",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lint": "tslint 'client/**/*.ts'",
    "bundle:prod": "node tools/builder.js --prod"
  },
  "keywords": [
    "test management",
    "test case management"
  ],
  "author": "Dmitry Zakharov",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "dependencies": {
    "@angular/common": "^2.2.1",
    "@angular/compiler": "^2.2.1",
    "@angular/core": "^2.2.1",
    "@angular/forms": "^2.2.1",
    "@angular/http": "^2.2.1",
    "@angular/platform-browser": "^2.2.1",
    "@angular/platform-browser-dynamic": "^2.2.1",
    "@angular/router": "^3.2.1",
    "@angular/upgrade": "^2.2.1",
    "angular-in-memory-web-api": "^0.1.16",
    "angular2-jwt": "^0.1.25",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "errorhandler": "^1.5.0",
    "express": "^4.14.0",
    "express-validator": "^2.21.0",
    "jwt-simple": "^0.5.1",
    "method-override": "^2.3.7",
    "mongoose": "^4.6.8",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "^2.3.2",
    "systemjs": "^0.19.41",
    "zone.js": "^0.7.0"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/es6-promise": "0.0.32",
    "@types/node": "^6.0.49",
    "@types/selenium-webdriver": "^2.53.35",
    "bluebird": "^3.4.6",
    "codelyzer": "^1.0.0-beta.4",
    "concurrently": "^3.1.0",
    "factory-girl": "^4.0.0-beta.7",
    "faker": "^3.1.0",
    "forever": "^0.15.3",
    "mocha": "^3.1.2",
    "mockgoose": "^6.0.8",
    "nodemon": "^1.11.0",
    "should": "^11.1.1",
    "supertest": "^2.0.1",
    "systemjs-builder": "^0.15.34",
    "tslint": "^4.0.1",
    "typescript": "^2.0.10",
    "typings": "^2.0.0",
    "versiony-cli": "^1.3.0",
    "webpack": "^1.13.3",
    "yargs": "^6.4.0",
    "yarn": "^0.17.8"
  }
}
