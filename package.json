{
  "name": "test-storage",
  "version": "0.16.52",
  "description": "Test Case Management System",
  "license": "GPLv3",
  "author": "Dmitry Zakharov",
  "private": true,
  "keywords": [
    "test management",
    "test case management"
  ],
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "postinstall": "./node_modules/.bin/ng build --prod",
    "dev": "concurrently \"ng serve --proxy-config proxy.conf.json\" \"nodemon server.js --exitcrash\" ",
    "lint": "ng lint",
    "test": "ng test --single-run && npm run test:backend",
    "pree2e": "webdriver-manager update",
    "e2e": "ng e2e",
    "test:backend": "./node_modules/.bin/_mocha ./test/server/ --recursive --check-leaks",
    "build": "ng build && nodemon server.js --exitcrash",
    "build:prod": "ng build --prod --aot && nodemon server.js --exitcrash",
    "extract": "ngx-translate-extract --input ./src --output ./i18n/*.json --clean --sort --format namespaced-json"
  },
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/cli": "1.0.4",
    "@angular/compiler-cli": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.1.3",
    "@ngx-translate/core": "6.0.1",
    "@ngx-translate/http-loader": "0.0.3",
    "angular-tree-component": "^3.7.2",
    "angular2-jwt": "^0.2.3",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "config": "^1.26.1",
    "errorhandler": "^1.5.0",
    "express": "^4.15.3",
    "express-validator": "^3.2.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^7.4.1",
    "method-override": "^2.3.9",
    "mongoose": "^4.10.0",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "ngx-bootstrap": "^1.6.6",
    "normalize-css": "^2.3.1",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "@types/jasmine": "2.5.47",
    "@types/node": "~7.0.21",
    "codelyzer": "~3.0.1",
    "concurrently": "^3.4.0",
    "database-updates": "^1.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^3.4.1",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.2",
    "should": "^11.2.1",
    "supertest": "^3.0.0",
    "ts-node": "~3.0.4",
    "tslint": "~5.2.0",
    "typescript": "~2.3.2"
  }
}