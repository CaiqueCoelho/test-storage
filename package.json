{
  "name": "test-storage",
  "version": "0.4.40",
  "description": "Test Case Management System",
  "main": "app.js",
  "private": true,
  "scripts": {
    "prestart": "npm run env NODE_ENV=production",
    "start": "concurrently \"tsc -w -p ./client\" \"nodemon app.js --exitcrash\" ",
    "prestartdev": "npm run env NODE_ENV=development",
    "startdev": "concurrently \"tsc -w -p ./client\" \"nodemon app.js --exitcrash\" ",
    "prestarttest": "concurrently \"tsc -p ./client\" \"tsc -p ./test\" ",
    "starttest": "nodemon app.js --exitcrash",
    "forever": "concurrently \"tsc -w -p ./client\" \"forever start -c nodemon app.js --exitcrash\" ",
    "testserver": "./node_modules/.bin/mocha ./test/server/ --recursive --check-leaks",
    "pretest": "npm run env NODE_ENV=test",
    "test": "./node_modules/.bin/mocha ./test/server/ --recursive --check-leaks",
    "webdriverupdate": "./node_modules/.bin/webdriver-manager update",
    "webdriverstart": "./node_modules/.bin/webdriver-manager start",
    "protractor": "npm run prestarttest & sleep 10 & npm run starttest & sleep 10 && protractor ./test/protractor.conf.js --suite all",
    "postinstall": "./node_modules/.bin/typings install",
    "typings": "typings",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lint": "tslint 'client/**/*.ts'",
    "bundle:prod": "node tools/builder.js --prod"
  },
  "keywords": [
    "test management",
    "test case management"
  ],
  "author": "Dmitry Zakharov",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "dependencies": {
    "@angular/common": "^4.0.0-rc.1",
    "@angular/compiler": "^4.0.0-rc.1",
    "@angular/core": "^4.0.0-rc.1",
    "@angular/forms": "^4.0.0-rc.1",
    "@angular/http": "^4.0.0-rc.1",
    "@angular/platform-browser": "^4.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^4.0.0-rc.1",
    "@angular/router": "^4.0.0-rc.1",
    "@angular/upgrade": "^4.0.0-rc.1",
    "angular-in-memory-web-api": "^0.3.0",
    "angular2-jwt": "^0.1.28",
    "body-parser": "^1.16.1",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "errorhandler": "^1.5.0",
    "express": "^4.14.1",
    "express-validator": "^3.1.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "jwt-simple": "^0.5.1",
    "method-override": "^2.3.7",
    "mongoose": "^4.8.5",
    "morgan": "^1.8.1",
    "normalize-css": "^2.3.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.2.0",
    "serve-favicon": "^2.4.1",
    "systemjs": "^0.19.46",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/core-js": "^0.9.35",
    "@types/es6-promise": "0.0.32",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/should": "^8.1.30",
    "@types/should-promised": "0.0.27",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codelyzer": "^2.0.0",
    "concurrently": "^3.3.0",
    "config": "^1.25.1",
    "database-updates": "^1.2.0",
    "factory-girl": "^4.2.1",
    "faker": "^4.1.0",
    "forever": "^0.15.3",
    "mocha": "^3.2.0",
    "mockgoose": "^7.0.0",
    "multer": "^1.3.0",
    "nodemon": "^1.11.0",
    "protractor": "^5.1.1",
    "should": "^11.2.0",
    "supertest": "^3.0.0",
    "systemjs-builder": "^0.16.3",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "typings": "^2.1.0",
    "versiony-cli": "^1.3.0",
    "webpack": "^2.2.1",
    "yargs": "^6.6.0",
    "yarn": "^0.21.3"
  }
}