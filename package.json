{
  "name": "test-storage",
  "version": "0.27.1",
  "description": "Test Case Management System",
  "license": "GPLv3",
  "author": "Dmitry Zakharov",
  "private": true,
  "keywords": [
    "test management",
    "test case management"
  ],
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "set NODE_ENV=production && ts-node server/index.ts",
    "heroku-prebuild": "npm i -g @angular/cli ts-node",
    "heroku-postbuild": "ng build --prod --aot --no-progress",
    "server:run": "nodemon",
    "hmr": "concurrently \"npm run server:run \" \"ng serve --hmr -e=hmr --progress=false --proxy-config proxy.conf.json\" ",
    "dev": "concurrently \"npm run server:run \" \"ng serve --progress=false --proxy-config proxy.conf.json\"  ",
    "lint": "ng lint --type-check",
    "test": "ng test --single-run --no-progress && npm run test:backend",
    "test:frontend": "./node_modules/.bin/istanbul cover ng test --single-run --no-progress",
    "test:backend": "./node_modules/.bin/_mocha ./test/server/**/*.spec.ts --compilers ts:ts-node/register --recursive --check-leaks -t 50000",
    "test:coverage": "ng test --code-coverage",
    "pree2e": "webdriver-manager update",
    "e2e": "ng e2e --no-progress --proxy-config proxy.conf.json",
    "build": "npm run client:build && npm run server:build",
    "server:build": "tsc -p ./server",
    "client:build": "ng build --prod --aot",
    "extract": "ngx-translate-extract --input ./src --output ./i18n/*.json --clean --sort --format namespaced-json"
  },
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@angularclass/hmr": "^2.1.3",
    "@auth0/angular-jwt": "^1.0.0-beta.9",
    "@ngx-translate/core": "8.0.0",
    "@ngx-translate/http-loader": "2.0.0",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.5",
    "@types/errorhandler": "0.0.31",
    "@types/express": "^4.0.37",
    "@types/jasmine": "^2.6.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/mocha": "2.2.43",
    "@types/morgan": "^1.7.33",
    "@types/node": "~8.0.33",
    "@types/supertest": "^2.0.3",
    "angular-tree-component": "^5.0.0",
    "angular2-notifications": "^0.7.8",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0-beta",
    "compression": "^1.7.1",
    "config": "^1.26.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.1",
    "database-updates": "^2.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.1",
    "express-validator": "^4.2.1",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^8.0.1",
    "mongoose": "^4.11.13",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "ngx-bootstrap": "^1.9.3",
    "ngx-clickout": "^1.0.2",
    "nodemon": "^1.12.1",
    "normalize-css": "^2.3.1",
    "rxjs": "^5.4.3",
    "ts-node": "~3.3.0",
    "typescript": "~2.5.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.5",
    "@angular/language-service": "^4.4.4",
    "@biesbjerg/ngx-translate-extract": "^2.3.2",
    "@types/chai": "^4.0.4",
    "@types/chai-http": "3.0.3",
    "@types/faker": "^4.1.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
