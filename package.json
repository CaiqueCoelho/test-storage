    "@angular/platform-browser-dynamic": "^4.0.3",
    "@angular/router": "^4.0.3",
    "@ngx-translate/core": "6.0.1",
    "@ngx-translate/http-loader": "0.0.3",
    "angular2-jwt": "^0.2.2",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "errorhandler": "^1.5.0",
    "express": "^4.15.2",
    "express-validator": "^3.1.3",
    "font-awesome": "^4.7.0",
